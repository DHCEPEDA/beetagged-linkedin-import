{
  "functions": {
    "serverless-functions/import-linkedin.js": {
      "maxDuration": 30
    },
    "serverless-functions/search-contacts.js": {
      "maxDuration": 15
    },
    "serverless-functions/get-contacts.js": {
      "maxDuration": 15
    },
    "serverless-functions/health-check.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/import/linkedin",
      "dest": "/serverless-functions/import-linkedin.js"
    },
    {
      "src": "/api/search/natural",
      "dest": "/serverless-functions/search-contacts.js"
    },
    {
      "src": "/api/contacts",
      "dest": "/serverless-functions/get-contacts.js"
    },
    {
      "src": "/health",
      "dest": "/serverless-functions/health-check.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri"
  }
}